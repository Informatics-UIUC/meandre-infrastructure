	<entry key="SELECT_JOB_CONSOLE">
			SELECT server_ID,ts,print 
			FROM meandre_job_console
			WHERE job_ID = ?
			ORDER BY ts
	</entry>
	<entry key="SELECT_JOB_LOG">
			SELECT server_ID,ts,level,log 
			FROM meandre_job_log
			WHERE job_ID = ?
